<script>
    $('#history').click(function (e) {
        e.preventDefault();
        var phoneNumber = localStorage.getItem("phone") || sessionStorage.getItem("phone");
        if (phoneNumber != null) {
            var form = $('<form action="/History/CinemaHotelHistory" method="post"></form>');
            form.append('<input type="hidden" name="phoneNumber" value="' + phoneNumber + '">');
            $('body').append(form);
            form.submit();
        }
    });
    $('#busHistory').click(function (e) {
        e.preventDefault();
        var phoneNumber = localStorage.getItem("phone");
        var sessionPhone = sessionStorage.getItem("phone");
        if (phoneNumber != null) {
            var form = $('<form action="/History/BusHistory" method="post"></form>');
            form.append('<input type="hidden" name="phoneNumber" value="' + phoneNumber + '">');
            $('body').append(form);
            form.submit();
        }
    });
</script>
